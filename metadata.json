{
    "name": "doubledog-koji",
    "version": "3.0.0",
    "author": "John Florian",
    "summary": "A module to manage components of the Koji build system",
    "license": "GPL-3.0",
    "source": "https://www.doubledog.org/git/puppet-koji.git",
    "project_page": "https://github.com/jflorian/doubledog-koji",
    "issues_url": "https://github.com/jflorian/doubledog-koji/issues",
    "operatingsystem_support": [
        {
            "operatingsystem": "CentOS",
            "operatingsystemmajrelease": "7"
        },
        {
            "operatingsystem": "Fedora",
            "operatingsystemrelease": [
                "27",
                "28",
                "29"
            ]
        }
    ],
    "dependencies": [
        {
            "name": "doubledog/apache",
            "version_requirement": ">= 1.0.0 < 3.0.0"
        },
        {
            "name": "doubledog/cron",
            "version_requirement": ">= 1.0.0 < 2.0.0"
        },
        {
            "name": "doubledog/openssl",
            "version_requirement": ">= 1.0.0 < 2.0.0"
        },
        {
            "name": "puppetlabs/concat",
            "version_requirement": ">= 4.0.0 < 5.0.0"
        },
        {
            "name": "puppetlabs/postgresql",
            "version_requirement": ">= 5.0.0 < 6.0.0"
        }
    ]
}
